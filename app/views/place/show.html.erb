<!-- Will show 1 place and eventually entries for that place -->


<h1> <%= @places["name"]%></h1> 

<ul>
    <%  for entry in @entries%>
    <li><%= entry["description"]%> <%= entry["occurred_on"]  %></li>
    <% end%>
  
</ul>

<p>
  <a href="/entry/new?place_id=<%=@places["id"]%>">New Post</a>
</p>


<p>
  <a href="/place">Back to All Places</a>
</p>

